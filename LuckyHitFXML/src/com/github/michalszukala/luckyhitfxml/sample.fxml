<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" prefHeight="300.0" prefWidth="600.0" style="-fx-background-color: #383838;" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.michalszukala.luckyhitfxml.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints maxWidth="126.0" minWidth="120.0" prefWidth="126.0" />
      <ColumnConstraints maxWidth="6.0" minWidth="0.0" prefWidth="0.0" />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Button fx:id="buttonLuckyHit" mnemonicParsing="false" onAction="#showLuckyHit" onKeyPressed="#pressEnter" prefHeight="26.0" prefWidth="125.0" style="-fx-font-weight: bold;" text="Lucky-Hit" GridPane.columnIndex="3">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="BLACK" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="buttonChart" mnemonicParsing="false" onAction="#showChart" prefHeight="25.0" prefWidth="126.0" text="Chart" visible="false" GridPane.columnIndex="3" GridPane.rowIndex="3">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Button>
      <Button fx:id="buttonResult" mnemonicParsing="false" onAction="#goToUrl" prefHeight="25.0" prefWidth="320.0" style="-fx-font-weight: bold;" text="Button" visible="false" GridPane.rowIndex="3">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Button>
      <TextField fx:id="textFieldUrl" onKeyPressed="#pressEnter" prefHeight="39.0" prefWidth="319.0" promptText="Type your starting page, and wait......">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
   </children>
   <effect>
      <ColorAdjust />
   </effect>
</GridPane>
